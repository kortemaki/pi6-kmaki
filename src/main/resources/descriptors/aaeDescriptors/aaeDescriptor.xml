<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>false</primitive>  
  <delegateAnalysisEngineSpecifiers>
    <delegateAnalysisEngine key="analysisEngineDescriptor">
      <import name="descriptors.aaeDescriptors.analysisEngineDescriptor"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="evaluationEngine">
      <import name="descriptors.aaeDescriptors.evaluationEngine"/>
    </delegateAnalysisEngine>
  </delegateAnalysisEngineSpecifiers>
  <analysisEngineMetaData>
    <name>aaeDescriptor</name>
    <description>The Aggregate Analysis Engine for pi6-kmaki.  Chains the analysis and evaluation aaes in sequence in order to evaluate the analysis system.</description>
    <version>1.0</version>
    <vendor/>
    <configurationParameters/>
    <configurationParameterSettings/>
    <flowConstraints>
      <fixedFlow>
        <node>analysisEngineDescriptor</node>
        <node>evaluationEngine</node>
      </fixedFlow>
    </flowConstraints>
    <typePriorities/>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs/>
        <outputs/>
        <languagesSupported/>
      </capability>
    </capabilities>
  <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration/>
</analysisEngineDescription>
